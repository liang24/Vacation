<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="/Content/css.css" charset="utf-8" />
    <script src="/Scripts/jquery-1.8.3.min.js" type="text/javascript"></script>
    <!--[if lte IE 6]>
    <script src="js/DD_belatedPNG_0.0.8a.js" type="text/javascript"></script>
        <script type="text/javascript">
            DD_belatedPNG.fix('div,ul,img,li,input,a,span,p');
        </script>
    <![endif]-->
</head>
<body class="@ViewBag.BodyClass">
    @RenderBody()
    <link href="/Scripts/artDialog/skins/default.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/artDialog/artDialog.js" type="text/javascript"></script>
    <script src="/Scripts/artDialog/plugins/iframeTools.js" type="text/javascript"></script>
    <script src="/Scripts/util.js" type="text/javascript"></script>
    <script src="/Scripts/common.js" type="text/javascript"></script>
    <!-- validform -->
    <link href="/Scripts/validform/style.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/validform/Validform_v5.3.1_min.js" type="text/javascript"></script>
    <!-- 分页 -->
    <link href="/Content/pagination.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/pagination/jquery.pagination.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.tmpl.min.js" type="text/javascript"></script>
    <!-- loadmask -->
    <link href="/Scripts/loadmask/jquery.loadmask.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/loadmask/jquery.loadmask.min.js" type="text/javascript"></script>
    @{
        if (IsSectionDefined("Script"))
        {
        @RenderSection("Script")
        }
    }
</body>
</html>
